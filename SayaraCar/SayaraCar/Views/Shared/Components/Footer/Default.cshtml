@model Bio 

<footer class="site-footer">
      <div class="footer-widgets">
        <div class="container">
          <div class="row">
            <div class="widget_text col-xl-3 col-lg-4 col-md-6">
              <div class="footer-widget">
                <a href="index.html" class="d-block mb-4" rel="home"
                  ><img
                    width="220"
                    src="~/assets/images/@Model.FooterLogo"
                    class="custom-logo"
                    alt="Sayara"
                /></a>
                <p class="mb-4">
                 @Model.FooterDesc
                </p>
                <div class="footer-social">
                  <ul class="mb-0">
                    <li>
                      <a href="@Model.Facebook"><i class="fab fa-facebook-f"></i></a>
                    </li>
                    <li>
                      <a href="@Model.Twitter"><i class="fab fa-twitter"></i></a>
                    </li>
                    <li>
                      <a href="@Model.Pinterest"><i class="fab fa-pinterest-p"></i></a>
                    </li>
                    <li>
                      <a href="@Model.Instagram"><i class="fab fa-instagram"></i></a>
                    </li>
                    <li>
                      <a href="@Model.LinkedIN"><i class="fab fa-linkedin"></i></a>
                    </li>
                  </ul>
                </div>
              </div>
            </div>
            <div class="col-xl-3 col-lg-4 col-md-6">
              <div class="footer-widget">
                <h5 class="widget-title">Customer Services</h5>
                <ul>
                  <li><a href="#">Contact Us</a></li>
                  <li><a href="#">Terms And Conditions</a></li>
                  <li><a href="#">About Us</a></li>
                  <li><a href="#">FAQ</a></li>
                  <li><a href="#">Delivery Information</a></li>
                </ul>
              </div>
            </div>
            <div class="col-xl-3 col-lg-4 col-md-6">
              <div class="footer-widget widget_nav_menu">
                <h5 class="widget-title">Information</h5>
                <ul>
                  <li class="menu-item"><a href="#">My account</a></li>
                  <li class="menu-item"><a href="#">Shop</a></li>
                  <li class="menu-item"><a href="#">become an affiliate</a></li>
                  <li class="menu-item"><a href="#">Special Offers</a></li>
                  <li class="menu-item">
                    <a href="#">Terms And Conditions</a>
                  </li>
                </ul>
              </div>
            </div>
            <div class="col-xl-3 col-lg-4 col-md-6">
              <div class="footer-widget">
                <h5 class="widget-title">Newsletter</h5>
                <p>
                  Subscribe to our Newsletter and get bonuses for the next
                  purchase
                </p>
                <form>
                  <input
                    type="email"
                    name="EMAIL"
                    placeholder="Your email address"
                    required
                  />
                  <input
                    class="mt-3 w-100 pl-5 pr-5"
                    type="submit"
                    value="Sign up"
                  />
                </form>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="copyright-bar">
        <div class="container">
          <div class="row justify-content-center">
            <div class="col-sm-7 text-left">
              <p>Copyright © 2020 Sayara All Rights Reserved.</p>
            </div>
            <div class="col-sm-5 currency-footer">
              <img src="~/assets/images/mastercard.png" alt="Image" />
              <img src="~/assets/images/visa.png" alt="Image" />
              <img src="~/assets/images/jcb.png" alt="Image" />
              <img src="~/assets/images/discover.png" alt="Image" />
              <img src="~/assets/images/paypal.png" alt="Image" />
            </div>
          </div>
        </div>
      </div>
    </footer>