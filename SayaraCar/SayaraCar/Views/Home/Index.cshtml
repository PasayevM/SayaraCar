    <section
      class="myBlogs pt-100 pb-70 bg-gray wow fadeInUp"
      data-wow-duration="2s"
      data-wow-delay="0.2s"
    >
      <div class="container">
        <div class="row pb-40">
          <div class="col-lg-6">
            <h3>Latest Article</h3>
          </div>
          <div class="col-lg-6">
            <div class="text-right">
              <a class="sayara-btn" href="#">Read Articles</a>
            </div>
          </div>
        </div>
      </div>
      <div class="container">
        <div class="row justify-content-center">
         
            @foreach (Blog blog in Model.Blogs)
                {
                        <div class="col-xl-3 col-lg-4 col-md-6 col-md-6">
                <div class="blog-item">
                  <div class="blog-thumb">
                    <a href="single.html">
                      <img
                    src="~/assets/images/@blog.Image"
                    alt="sayara"
                  />
                    </a>
                    <span class="date">@blog.Date</span>
                  </div>
                  <div class="blog-content">
                    <div class="blog-meta">
                      @Html.Raw(blog.Title)
                    </div>
                    <h4>
                      <a href="single.html">@blog.Name</a>
                    </h4>
                    <p>
                      @blog.Description
                    </p>
                  </div>
                </div>
              </div>
                }
        </div>
      </div>
    </section>
    <!-- ********************     Blogs End     ********************* -->
    <!-- *************************************************************** -->


