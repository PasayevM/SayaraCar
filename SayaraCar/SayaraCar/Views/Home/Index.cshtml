@model   HomeVM
<main>
        <!-- ********************     Banner Start    ********************* -->
    <section
      class="banner"
      style="
        background: url(../assets/images/@Model.Banners.Image);
      "
    >
      <div class="container">
        <div class="row">
          <div class="col-lg-12">
            <div class="banner-content">
              <h1>
                @Html.Raw(Model.Banners.Title)
              </h1>
              <p>@Model.Banners.Description</p>
            </div>
            <div class="brands_form">
              <form method="GET" action="#">
                <ul class="list-inline">
                  <li class="list-inline-item">
                    <div class="sayara-custom-select">
                      <select name="pa_car-brand" id="car-brand">
                        <option value="-1" disabled selected>
                          Brand Chevrolet
                        </option>
                        @foreach (BannerModel bannerModel in Model.BannerModels)
                           {
                                   <option value="@bannerModel.Choice">@bannerModel.Choice</option>
                           }
                      </select>
                    </div>
                  </li>
                  <li class="list-inline-item">
                    <div class="sayara-custom-select">
                      <select name="pa_car-type" id="car-type">
                        <option value="-1" disabled selected>Type</option>
                        @foreach (BannerType bannerType in Model.BannerTypes)
                        {
                                <option value="@bannerType.Choice">@bannerType.Choice</option>
                        }
                      </select>
                    </div>
                  </li>
                  <li class="list-inline-item">
                    <div class="sayara-custom-select">
                      <select name="pa_color" id="color">
                        <option value="-1" disabled selected>Color</option>
                        @foreach (BannerColor bannerColor in Model.BannerColors)
                        {
                                    <option value="@bannerColor.Choice">@bannerColor.Choice</option>
                        }
                      </select>
                    </div>
                  </li>
                  <li class="list-inline-item">
                    <input type="submit" value="Search" />
                    <input type="hidden" name="post_type" value="product" />
                  </li>
                </ul>
              </form>
            </div>
          </div>
        </div>
      </div>
    </section>
    <!-- ********************     Banner End    ********************* -->
    <!-- ************************************************************** -->


    <!-- ********************     Service Start    ********************* -->
    <section class="myService pt-30 pb-30 bg-gray">
      <div class="container">
        <div class="row">
          @foreach (Service service in Model.Services)
                {
                        <div class="col-md-3">
                 <div
                   class="iconbox-item wow fadeInLeft"
                   data-wow-duration="2s"
                   data-wow-delay="0.2s"
                 >
                  <div><i class="@service.Icon"></i></div>
                   <div>
                    <h5>@service.Title</h5>
                    <p>@service.SubTitle</p>
                   </div>
                  </div>
                 </div>
                }
        </div>
      </div>
    </section>
    <!-- ********************     Service End    ********************* -->
    <!-- ************************************************************** -->

    <!-- ********************     Preview Start   ********************* -->
    <section class="myPreview pt-60 pb-60">
      <div class="container">
        <div class="row">
          @foreach (Preview preview in Model.Previews)
                {
                        <div class="col-lg-4">
                <section
              class="ad-banner horizontal wow fadeInLeft"
              data-wow-duration="2s"
              data-wow-delay="0.2s"
              style="
                padding: 30px;
                background-image: url(../assets/images/@preview.Image);
              "
            >
                  <div class="container">
                    <div class="row justify-content-between">
                      <div class="col-md-12 text-left">
                        <p>@preview.Title</p>
                        <h2 class="mb-5">@preview.Description</h2>
                        <a class="sayara-btn" href="shop.html">İndi al</a>
                      </div>
                    </div>
                  </div>
                </section>
              </div>
                }
        </div>
      </div>
    </section>
    <!-- ********************      Preview End     ********************* -->
    <!-- *************************************************************** -->


    <!-- ********************     Products Start   ********************* -->
    <section
      class="myProduct pt-100 pb-145 bg-gray wow fadeInUp"
      data-wow-duration="2s"
      data-wow-delay="0.2s"
    >
      <div class="container">
        <div
          class="product-filter d-flex justify-content-center"
          id="product-filter"
        >
          <h4>Ən son Mallar</h4>
        </div>

        <div class="tab-content">
          <div class="container tab-pane p-0 active">
            <div
              class="product-items"
              data-slick='{"slidesToShow": 4, "slidesToScroll": 4}'
            >
              @foreach (Product product in Model.Products)
                        {
                                <div>
                    <div class="product-item">
                      <div class="product-item-image">
                        <a href="shop-single.html">
                          <img src="~/assets/images/@product.Image" alt="Image" />
                        </a>
                      </div>
                      <div class="product-item-content">
                        <a href="shop.html">@product.Name</a>
                        <a href="shop-single.html">
                          <h5>@product.Description</h5>
                        </a>
                        <ul class="list-inline mb-0">
                          <li class="list-inline-item">
                            <p class="price">
                              <span class="woocommerce-Price-amount amount"
                            >@Html.Raw(product.Price)
                            </p>
                          </li>
                          <li class="list-inline-item float-right">
                            <div class="star-rating">
                             @Html.Raw(product.Raiting)
                            </div>
                          </li>
                        </ul>
                      </div>
                      <div class="product-item-overlay">
                        <a href="cart.html" class="button">Add to cart</a>
                      </div>
                      <a class="ajax-quick-view-popup" href="quick-view-popup.html">
                        <i class="fas fa-search-plus"></i>
                      </a>
                    </div>
                  </div>
                        }
          </div>
        </div>
      </div>
    </section>
